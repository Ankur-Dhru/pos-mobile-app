{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn  react-native run-android --active-arch-only   ",
    "ios": "yarn  react-native run-ios   --simulator='iPhone 11 Pro'  ",
    "iosIpad": "yarn  react-native run-ios --simulator='iPad Pro (9.7-inch)' ",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "cache": "yarn cache clean",
    "clean all": "rm -rf node_modules && yarn cache clean && yarn && rm -fr $TMPDIR/haste-map-* && rm -rf $TMPDIR/metro-cache && yarn start -- --reset-cache",
    "upgrade": "yarn upgrade --latest",
    "link": "react-native link react-native-vector-icons --platforms android",
    "force update on git": "git push -f origin master",
    "gradle clean": "cd android && ./gradlew clean"
  },
